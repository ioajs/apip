{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "strict": true,
    "declaration": true,
    // "emitDeclarationOnly": true,
    // "noEmit": true,
    "paths": {
      "apip": [
        "./main/index"
      ],
      "apip/imports": [
        "./main/imports"
      ],
      "apip/http": [
        "./http/index"
      ],
      "apip/nats": [
        "./nats/index"
      ]
    },
  }
}